});

function indexOf(arr, value) {
    if (arr.indexOf(value) < 0) {
        return (false)
    } else {
        return (true)
    }
};

function toFixed(num) {
    return (num.toFixed(2))
};
module.exports.indexOf = indexOf;
module.exports.toFixed = toFixed;
