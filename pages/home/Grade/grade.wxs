        case "03":
            return ("期末考试");
        case "04":
            return ("实验环节");
        case "05":
            return ("实践环节");
        case "06":
            return ("平时测验");
        case "07":
            return ("日常考勤");
        case "08":
            return ("课堂表现");
        case "09":
